{
  "name": "banker-assistant",
  "private": true,
  "scripts": {
    "backend": "python -m uvicorn backend.app:app --reload --host 0.0.0.0 --port 8000",
    "frontend": "cd frontend-react && npm run dev",
    "dev": "concurrently -n backend,frontend -c blue,green \"npm run backend\" \"npm run frontend\""
  },
  "devDependencies": {
    "concurrently": "^9.1.2"
  }
}
